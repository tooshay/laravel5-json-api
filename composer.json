{
    "name": "nilportugues/laravel5-json-api",
    "type": "library",
    "description": "Laravel 5 JSON API Transformer Package",
    "keywords": ["laravel", "laravel5", "lumen", "json", "api", "jsonapi", "serializer", "transformer", "psr7", "response"],
    "homepage": "http://nilportugues.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Nil Portugués Calderó",
            "email": "contact@nilportugues.com",
            "role": "Project Lead Developer"
        }
    ],
    "require": {
        "nilportugues/json-api": "dev-master",
        "symfony/psr-http-message-bridge" : "^0.2|^0.3|^1.0",
        "nilportugues/serializer-eloquent": "~1.0"
    },
    "require-dev": {
        "laravel/laravel": "5.*",
        "laravel/lumen": "^5.2",
        "phpunit/phpunit": "4.*",
        "friendsofphp/php-cs-fixer": "^1.10"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tooshay/json-api"
        }
    ],
    "autoload": {
        "psr-4": {
            "NilPortugues\\Laravel5\\JsonApi\\" : "src/NilPortugues/Laravel5/JsonApi/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NilPortugues\\Tests\\" : "tests/NilPortugues/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
